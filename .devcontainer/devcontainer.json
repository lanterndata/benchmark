{
    "name": "Lantern Benchmarking",
    "dockerFile": "../Dockerfile",
    "context": "..",
    "runArgs": [
        "--env-file",
        ".env",
    ],
    "mounts": [
        "source=${localWorkspaceFolder},target=/app,type=bind,consistency=cached",
        "source=${localWorkspaceFolder}/db,target=/var/lib/postgresql/data,type=bind,consistency=cached"
    ],
    "overrideCommand": false,
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.autopep8",
                "ms-python.python"
            ],
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash"
            }
        }
    },
    "containerEnv": {
        "PYTHONPATH": "/workspaces/benchmark"
    }
}