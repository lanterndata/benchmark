{
    "name": "Lantern Benchmarking",
    "dockerFile": "../docker/Dockerfile.dev",
    "context": "..",
    "runArgs": [
        "--cap-add=SYS_PTRACE",
        "--security-opt",
        "seccomp=unconfined",
        "-e",
        "POSTGRES_PASSWORD=postgres"
    ],
    "mounts": [
        "source=${localWorkspaceFolder},target=/benchmark,type=bind,consistency=cached"
    ],
    "overrideCommand": false,
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.autopep8",
                "ms-python.python"
            ],
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash"
            }
        }
    }
}